                 name='데이터'))\n","\n","# 추세선 추가 (OLS)\n","X = year_df['폐업건수종합']\n","Y = year_df['수입(건수)']\n","X = sm.add_constant(X)  # 상수 추가\n","model = sm.OLS(Y, X).fit()\n","trendline = model.predict(X)\n","\n","fig.add_trace(go.Scatter(x=year_df['폐업건수종합'], y=trendline,\n","                         mode='lines',\n","                         line=dict(color='blue'),  # 선 색깔 설정\n","                         name='OLS 추세선'))\n","\n","# 레이아웃 업데이트\n","fig.update_layout(\n","    title='폐업건수종합과 수입(건수)의 상관관계',\n","    xaxis_title='폐업건수종합',\n","    yaxis_title='수입(건수)',\n","    template='plotly_white',  # 템플릿 설정\n","    yaxis=dict(\n","        tickformat='~s'  # 숫자를 k, m 단위로 표시\n","    )\n",")\n","\n","# 그래프 출력\n","fig.show()\n","\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":542},"id":"5MXPWXLA01Is","executionInfo":{"status":"ok","timestamp":1719376613136,"user_tz":-540,"elapsed":367,"user":{"displayName":"조인영","userId":"05477539260154813184"}},"outputId":"dd668d13-7a73-4f74-bba1-9cc43c53f955"},"execution_count":null,"outputs":[{"output_type":"display_data","data":{"text/html":["<html>\n","<head><meta charset=\"utf-8\" /></head>\n","<body>\n","    <div>            <script src=\"https://cdnjs.cloudflare.com/ajax/libs/mathjax/2.7.5/MathJax.js?config=TeX-AMS-MML_SVG\"></script><script type=\"text/javascript\">if (window.MathJax && window.MathJax.Hub && window.MathJax.Hub.Config) {window.MathJax.Hub.Config({SVG: {font: \"STIX-Web\"}});}</script>                <script type=\"text/javascript\">window.PlotlyConfig = {MathJaxConfig: 'local'};</script>\n","        <script charset=\"utf-8\" src=\"https://cdn.plot.ly/plotly-2.24.1.min.js\"></script>                <div id=\"ecc4f1de-ca1c-4642-9284-dbb1bd418015\" class=\"plotly-graph-div\" style=\"height:525px; width:100%;\"></div>            <script type=\"text/javascript\">                                    window.PLOTLYENV=window.PLOTLYENV || {};                                    if (document.getElement